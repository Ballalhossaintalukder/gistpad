{
	"name": "gistfs",
	"displayName": "GistFS",
	"description": "Browse, edit and fork GitHub Gists, directly within Visual Studio Code",
	"publisher": "vsls-contrib",
	"version": "0.0.5",
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/vsls-contrib/gist-fs"
	},
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"Collaboration",
		"Sharing",
		"Remote"
	],
	"activationEvents": [
		"onCommand:gistfs.deleteGist",
		"onCommand:gistfs.forkGist",
		"onCommand:gistfs.newPublicGist",
		"onCommand:gistfs.newSecretGist",
		"onCommand:gistfs.openGist",
		"onCommand:gistfs.openGistWorkspace",
		"onCommand:gistfs.starredGists",
		"onCommand:gistfs.signOut",
		"onFileSystem:gist",
		"onView:gistfs.gists"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "gistfs.deleteGist",
				"title": "Delete Gist",
				"category": "GistFS"
			},
			{
				"command": "gistfs.forkGist",
				"title": "Fork Gist",
				"category": "GistFS"
			},
			{
				"command": "gistfs.newPublicGist",
				"title": "New Gist",
				"category": "GistFS",
				"icon": {
					"dark": "images/dark/add.svg",
					"light": "images/light/add.svg"
				}
			},
			{
				"command": "gistfs.newSecretGist",
				"title": "New Secret Gist",
				"category": "GistFS",
				"icon": {
					"dark": "images/dark/add-secret.svg",
					"light": "images/light/add-secret.svg"
				}
			},
			{
				"command": "gistfs.loadGists",
				"title": "Refresh Gists",
				"category": "GistFS",
				"icon": {
					"dark": "images/dark/refresh.svg",
					"light": "images/light/refresh.svg"
				}
			},
			{
				"command": "gistfs.openGist",
				"title": "Open Gist",
				"category": "GistFS"
			},
			{
				"command": "gistfs.openGistWorkspace",
				"title": "Open Gist as Workspace",
				"category": "GistFS"
			},
			{
				"command": "gistfs.signIn",
				"title": "Sign In",
				"category": "GistFS"
			},
			{
				"command": "gistfs.signOut",
				"title": "Sign Out",
				"category": "GistFS"
			},
			{
				"command": "gistfs.starredGists",
				"title": "Starred Gists",
				"category": "GistFS"
			},
			{
				"command": "gistfs.unstarGist",
				"title": "Unstar Gist",
				"category": "GistFS"
			},
			{
				"command": "gistfs.addFile",
				"title": "Add File(s)",
				"category": "GistFS"
			},
			{
				"command": "gistfs.deleteFile",
				"title": "Delete File",
				"category": "GistFS"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "gistfs.loadGists",
					"when": "false"
				},
				{
					"command": "gistfs.unstarGist",
					"when": "false"
				},
				{
					"command": "gistfs.addFile",
					"when": "false"
				},
				{
					"command": "gistfs.deleteFile",
					"when": "false"
				},
				{
					"command": "gistfs.signIn",
					"when": "gistfs:state != SignedIn"
				},
				{
					"command": "gistfs.signOut",
					"when": "gistfs:state != SignedOut"
				}
			],
			"view/title": [
				{
					"command": "gistfs.newPublicGist",
					"when": "view == gistfs.gists && gistfs:state == SignedIn",
					"group": "navigation@1"
				},
				{
					"command": "gistfs.newSecretGist",
					"when": "view == gistfs.gists && gistfs:state == SignedIn",
					"group": "navigation@2"
				},
				{
					"command": "gistfs.loadGists",
					"when": "view == gistfs.gists && gistfs:state == SignedIn",
					"group": "navigation@3"
				}
			],
			"view/item/context": [
				{
					"command": "gistfs.openGist",
					"when": "viewItem == gists.gist",
					"group": "base@1"
				},
				{
					"command": "gistfs.openGistWorkspace",
					"when": "viewItem == gists.gist",
					"group": "base@2"
				},
				{
					"command": "gistfs.addFile",
					"when": "viewItem == gists.gist",
					"group": "manage@1"
				},
				{
					"command": "gistfs.deleteFile",
					"when": "viewItem == gists.gist.file",
					"group": "manage@1"
				},
				{
					"command": "gistfs.deleteGist",
					"when": "viewItem == gists.gist",
					"group": "manage@2"
				},
				{
					"command": "gistfs.unstarGist",
					"when": "viewItem == starredGists.gist"
				},
				{
					"command": "gistfs.forkGist",
					"when": "viewItem == starredGists.gist"
				}
			]
		},
		"views": {
			"explorer": [
				{
					"id": "gistfs.gists",
					"name": "Gists"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/keytar": "^4.4.2",
		"@types/node": "^8.10.25",
		"tslint": "^5.8.0",
		"typescript": "^3.1.4",
		"vscode": "^1.1.25"
	},
	"dependencies": {
		"gists": "^2.0.0",
		"mobx": "^5.14.2"
	}
}
